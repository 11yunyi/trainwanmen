    "    for i in range(3):\n",
    "        if i==car_in_door:\n",
    "            door[car_in_door]=0\n",
    "        else:\n",
    "            door[i]=1\n",
    "    #choose the door 0:\n",
    "    choose_a_door=0\n",
    "    #open a other door with a sheep:\n",
    "    open_a_other_door=random.randrange(1,3)\n",
    "    not_opened_door=3-open_a_other_door\n",
    "    if (door[open_a_other_door]==1):\n",
