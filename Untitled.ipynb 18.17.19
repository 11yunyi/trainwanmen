    "#掷骰子喝酒问题\n",
    "def end_Expection(end_of_the_game=False):\n",
    "    cup=[False]*6\n",
    "    times=1\n",
    "    all_cup_filled=[True]*6\n",
    "    while end_of_the_game == False:\n",
    "        pointer=random.randrange(0,6)\n",
    "        if cup[pointer]==False:\n",
    "            cup[pointer]=True\n",
    "            times+=1\n",
    "            if all_cup_filled==cup:\n",
    "                end_of_the_game = True\n",
    "        else:\n",
    "            cup[pointer]=False\n",
    "            \n",
    "            \n",
    "                \n",
    "        \n",
    "    return times\n",
    "    \n",
