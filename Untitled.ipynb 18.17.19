    "def find_key_index(lst):\n",
    "    i=0\n",
    "    if len(lst) < 3:\n",
    "        return -1\n",
    "    else:\n",
    "        while i<len(lst):\n",
    "            if sum(lst[:i])==sum(lst[i+1:len(lst)]):\n",
    "                return i\n",
    "            else:\n",
    "                i+=1\n",
    "        return -1"
