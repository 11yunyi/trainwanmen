    "@pysnooper.snoop()\n",
    "def searchInsert(nums, target):\n",
    "    if len(nums) == 0:\n",
    "        pos = 0\n",
    "        return pos\n",
    "    left,right = 0,len(nums)-1\n",
    "    while left+1 < right:\n",
    "        mid = left + (right - left)//2\n",
    "        if target == nums[mid]:\n",
    "            pos = mid\n",
    "        if target < nums[mid]:\n",
    "            right = mid\n",
    "        else:\n",
    "            left = mid\n",
    "    if nums[left] >= target:\n",
    "         pos = left\n",
    "    if nums[right] >= target:\n",
    "        pos = right\n",
    "    else:\n",
    "        pos = right+1\n",
    "    return pos"
   ]
