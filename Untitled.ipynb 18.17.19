    "def twoSum(numbers, target):\n",
    "    for i in range(len(numbers)):\n",
    "        if i<len(numbers):\n",
    "            if target>numbers[i] and target<numbers[i+1]:\n",
    "                right=i \n",
    "        else:\n",
    "            right = i\n",
    "    a=0\n",
    "    while a<right:\n",
    "        if numbers[a]+numbers[right] > target:\n",
    "            a+=1\n",
    "        elif numbers[a]+numbers[right] < target:\n",
    "            right=right-1\n",
    "        else:\n",
    "            return [a+1,right+1]"
   ]
