    "from collections import Counter\n",
    "#@pysnooper.snoop()\n",
    "def neighbors(cell):\n",
    "    (x,y)=cell\n",
    "    return[(x-1,y-1),(x,y-1),(x+1,y-1),\n",
    "           (x-1,y),          (x+1,y),\n",
    "           (x-1,y+1),(x,y+1),(x+1,y+1)]\n",
    "\n",
    "#@pysnooper.snoop()\n",
    "def neighbors_counts(world):\n",
    "\n",
    "   \n",
    "    return Counter(nb for cell in world for nb in neighbors(cell))\n",
    "@pysnooper.snoop()\n",
    "def next_generation(world):\n",
    "    possible_cells=counts=neighbors_counts(world)\n",
    "    return{cell for cell in possible_cells if (counts[cell]==3) or (counts[cell]==2 and cell in world)}"
   ]
